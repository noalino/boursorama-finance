{
  "name": "boursorama-finance",
  "version": "1.0.1",
  "description": "A wrapper to get financial quotes from the Boursorama website.",
  "author": "Beno√Æt Gelineau <bgelineau.dev@gmail.com> (https://www.benoitgelineau.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/benoitgelineau/boursorama-finance.git"
  },
  "homepage": "https://github.com/benoitgelineau/boursorama-finance#readme",
  "bugs": {
    "url": "https://github.com/benoitgelineau/boursorama-finance/issues"
  },
  "license": "MIT",
  "keywords": [
    "boursorama",
    "finance",
    "financial",
    "data",
    "stock",
    "price",
    "quote",
    "historical",
    "funds",
    "library"
  ],
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "cross-fetch": "^3.1.5"
  }
}
